component("x11") {
  output_name = "x11_window"

  deps = [
    "//base",
    "//skia",
    "//ui/events",
    "//ui/events/platform/x11",
    "//ui/gfx/x",
    "//ui/platform_window",
  ]

  configs += [ "//build/config/linux:x11" ]

  defines = [ "X11_WINDOW_IMPLEMENTATION" ]

  sources = [
    "x11_window.cc",
    "x11_window.h",
    "x11_window_export.h",
  ]
}
