// Spec: https://w3c.github.io/webcomponents/spec/shadow/#shadowrootinit-dictionary

enum ShadowRootMode { "open", "closed" };

dictionary ShadowRootInit {
    ShadowRootMode mode;
    [RuntimeEnabled=ShadowRootDelegatesFocus] boolean delegatesFocus;
};
