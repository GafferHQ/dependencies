enum AppBannerPromptOutcome { "accepted", "dismissed" };

[
    GarbageCollected,
    RuntimeEnabled=AppBanner,
] interface AppBannerPromptResult {
    readonly attribute DOMString platform;
    readonly attribute AppBannerPromptOutcome outcome;
};
